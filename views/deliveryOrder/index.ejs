<html>
    <head>
        <title>Warehouse In Out system</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    </head>
    <body>
        <div class="container">
            <a href="/" class="btn btn-primary">index</a>
            <div class="row my-2 border-bottom border-dark">
                <div class="col fw-bold fs-5">
                    id
                </div>
                <div class="col fw-bold fs-5">
                    type
                </div>
                <div class="col fw-bold fs-5">
                    company
                </div>
                <div class="col fw-bold fs-5">
                    phone
                </div>
                <div class="col fw-bold fs-5">
                    delivery check
                </div>
                <div class="col fw-bold fs-5">
                    delivery user
                </div>
                <div class="col fw-bold fs-5">
                    delivery at
                </div>
                <div class="col fw-bold fs-5">
                    edit
                </div>
            </div>
            <% for(data of datas){ %>
                <div class="row py-2 border-bottom border-dark">
                    <div class="col">
                        <%= data.delivery_id %>
                    </div>
                    <div class="col">
                        <%= data.type %>
                    </div>
                    <div class="col">
                        <%= data.company %>
                    </div>
                    <div class="col">
                        <%= data.phone %>
                    </div>
                    <div class="col">
                        <%= data.delivery_check ? "finish": "not finish" %>
                    </div>
                    <div class="col">
                        <%= data.delivery_user %>
                    </div>
                    <div class="col">
                        <%= data.delivery_at %>
                    </div>
                    <div class="col">
                        <a href="/stock/<%= data._id %>" class="btn btn-primary">edit</a>
                        <a href="/stock/delete/<%= data._id %>" class="btn btn-warning">delete</a>   
                    </div>
                </div>
            <% } %>
        </div>
    </body>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</html>