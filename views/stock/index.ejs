<html>
    <head>
        <title>Warehouse In Out system</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    </head>
    <body>
        <div class="container">
            <a href="/" class="btn btn-primary">index</a>
            <div class="row my-2 border-bottom border-dark">
                <div class="col fw-bold fs-5">
                    id
                </div>
                <div class="col fw-bold fs-5">
                    area
                </div>
                <div class="col fw-bold fs-5">
                    name
                </div>
                <div class="col fw-bold fs-5">
                    edit
                </div>
            </div>
            <% for(data of datas){ %>
                <div class="row py-2 border-bottom border-dark">
                    <div class="col">
                        <%= data.stock_id %>
                    </div>
                    <div class="col">
                        <%= data.area %>
                    </div>
                    <div class="col">
                        <%= data.name %>
                    </div>
                    <div class="col">
                        <a href="/stock/info?stock_id=<%= data._id %>" class="btn btn-primary">edit</a>
                        <a href="/stock/delete?stock_id=<%= data._id %>" class="btn btn-warning">delete</a>   
                    </div>
                </div>
            <% } %>
        </div>
    </body>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</html>